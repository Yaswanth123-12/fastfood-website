            border-radius: 6px;
            /* box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px; */
            /* align-items: center; */
        }

        .row1 {
            display: flex;
            justify-content: center;
            align-items: center;
            margin-top: 15px;
            gap: 25px;
            flex-wrap: wrap;
        }

        .cart {
            border: none;
            background-color: #dd2803;
            color: white;
            width: 120px;
            height: 35px;
        }

        .inc-dec {
            display: flex;
            gap: 10px;
        }

        .increment button {
            background: none;
            border: none;
            font-size: 30px;
            color: white
        }

        .increment span {
            color: white;
            font-size: 20px;
        }

        .increment {
            background: #dd2803;
            width: 80px;
            height: 35px;
            display: flex;
            justify-content: space-evenly;
            align-items: center;
        }

        .card {
            transition: 0.5s all ease;
        }

        .card img {
            height: 16rem;
            width: 20rem;
        }

        .card:hover {
            transform: scale(1.05);
            cursor: pointer;
        }

        .main1 {
