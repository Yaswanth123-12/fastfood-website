    <script>
        let count = 0;
        let a = 0;
        function increment(id) {
            count = parseInt(document.getElementById(id).textContent);
            count++;
            document.getElementById(id).textContent = count;
        }
        function decrement(id) {
            count = parseInt(document.getElementById(id).textContent);
            if (count > 0) {
                count--;
                document.getElementById(id).textContent = count;
            }
        }
        document.addEventListener("DOMContentLoaded", function () {
            const cartSidebar = document.getElementById("cartSidebar");
            const viewCartButton = document.getElementById("View-cart");
            const closeSidebarButton = document.getElementById("closeSidebar");
            // Function to open the cart sidebar
            function openCartSidebar() {
                cartSidebar.classList.add("active");
            }

            function closeCartSidebar() {
                cartSidebar.classList.remove("active");
            }
            viewCartButton.addEventListener("click", openCartSidebar);

            closeSidebarButton.addEventListener("click", closeCartSidebar);

        })

        var list = [];
        function AddCart(ItemName, Cost) {
            if (count == 0) {
                alert('Please Select Quantity');
                // document.getElementById("message1").innerHTML="Please Select Quantity";
            }
            else {
                document.getElementById("s11").innerHTML = count + a;
                list.push({ Item: ItemName + " x" + count, Price: Cost * count });
                a = count;
                count = 0;

                UpdateCart();
            }
        }
        function UpdateCart() {
            var cartContainer = document.getElementById("Cart-Container");
            cartContainer.innerHTML = "";
            let totalAmount = 0;
            list.forEach(item => {
                totalAmount += parseFloat(item.Price);
                cartContainer.innerHTML += `<div class="cart-item" style="display:flex; justify-content:space-between;">
                                    <span >${item.Item}</span>
                                    <span class="cart-price" >$${item.Price}</span>
                              </div>`;
            });

            cartContainer.innerHTML += `<div class="cart-item  cart-total" style="margin-top:20px">
                                   <span><strong><center>Total:<strong></span>
                                   <span class="cart-price">$${totalAmount}</center></span>
                            </div>`;
            sessionStorage.setItem("totalamount", totalAmount);
        }
        function clearCart() {
            list.length = 0;
            document.getElementById("s11").innerHTML = 0;
            UpdateCart();
        }
        function Pay() {
            window.location.href = "/payment";

        }

        // Retrieve the 'username' cookie
        document.addEventListener("DOMContentLoaded", function () {
            const username = document.cookie
                .split('; ')
                .find(row => row.startsWith('username='))
                .split('=')[1];

            if (username) {
                document.getElementById("user").innerHTML = username;
            }
        });
        function logout() {
            // Set a logout flag in a cookie
            document.cookie = 'username=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;';
            document.cookie = 'logoutFlag=true; path=/';
            // Redirect to the home page
            window.location.href = "/home"; 
        }
    </script>
