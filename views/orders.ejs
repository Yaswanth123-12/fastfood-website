            text-align: center;
            color: #dd2803;
            margin-top: auto;
        }

        #button {
            background-color: transparent;
            color: black;
            border: none;
        }

        .container {
            /* margin-top: 30px; */
            padding-top: 30px;
            display: flex;
            gap: 30px;
            justify-content: center;
            align-items: center;
        }

        .items::-webkit-scrollbar {
            display: none;
            /* Hide Chrome, Safari, and Opera scrollbar */
        }

        .items {
            margin-top: 13px;
            border-bottom: 1px solid #00000033;
            display: flex;
            gap: 30px;
            width: 459px;
            overflow: auto;
            white-space: nowrap;
            padding: 10px;
        }

        .items p {
            cursor: pointer;
        }

        .empty {
